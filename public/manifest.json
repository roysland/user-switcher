{
    "manifest_version": 3,
    "name": "Vieri User Switcher",
    "description": "Switch between different roles and users",
    "version": "0.0.1",
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://*.localhost*"
    ],
    "permissions": [
      "cookies"
    ]
  }